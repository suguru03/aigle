{
  "name": "aigle",
  "version": "1.4.2",
  "description": "Aigle is an ideal Promise library, faster and more functional than other Promise libraries",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover mocha --report lcovonly -- -R spec ./test --recursive && codecov",
    "test-es6": "ENTRY=./dist/aigle.js mocha ./test --recursive --reporter nyan --require mocha-clean",
    "test-es6-min": "ENTRY=./dist/aigle.min.js mocha ./test --recursive --reporter nyan --require mocha-clean",
    "test-es5": "ENTRY=./dist/aigle-es5.js mocha ./test --recursive --reporter nyan --require mocha-clean",
    "test-es5-min": "ENTRY=./dist/aigle-es5.min.js mocha ./test --recursive --reporter nyan --require mocha-clean"
  },
  "keywords": [
    "aigle",
    "promise",
    "async"
  ],
  "files": [
    "README.md",
    "index.js",
    "lib/",
    "dist/"
  ],
  "author": "Suguru Motegi",
  "license": "MIT",
  "devDependencies": {
    "babili": "0.1.2",
    "benchmark": "^2.1.1",
    "bluebird": "^3.5.0",
    "browserify": "^14.1.0",
    "buble": "^0.15.2",
    "codecov": "^2.1.0",
    "docdash": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-git": "^2.0.0",
    "gulp-tag-version": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "mocha.parallel": "^0.15.0",
    "neo-async": "^2.0.1",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "uglify-js": "^3.0.0"
  },
  "dependencies": {
    "aigle-core": "^1.0.0"
  }
}
